<!DOCTYPE html>
<html lang='{{request.locale_name}}'>
    <head>
        <meta charset='utf-8'>
        <meta name='viewport' content='width=device-width, initial-scale=1.0'>
        <meta name='description' content='Synergy Web Visualization'>
        <meta name='author' content='Pearce & Sarah'>
        <link rel='shortcut icon' href='{{request.static_url("synergy:static/pyramid-16x16.png")}}'>

        <!-- The created CSS bundle will be injected here based on webpack/base.config.js -->
        <link rel='stylesheet' href='{{request.static_url("synergy:static/app/bundle.css")}}'>

        {% block customcss %}
        {% endblock customcss %}
        <title>Synergy Visualization</title>
    </head>
    <body>
        <div id='app'></div>
        {% block customcontent %}
        {% endblock customcontent %}
        <!-- The created JS bundle will be injected here based on webpack/base.config.js -->
        <script src='{{request.static_url("synergy:static/app/bundle.js")}}'></script>
        {% block customjs %}
        {% endblock customjs %}
    </body>
</html>